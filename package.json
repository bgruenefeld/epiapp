{
  "name": "epiweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://bgruenefeld.github.io/epiapp",
  "proxy": "https://mein-backend.onrender.com",
  "scripts": {
    "debug": "ls -lah node_modules/@types/react >> debug.log && cat debug.log",
    "dev": "vite",
    "prebuild": "echo NODE_ENV=$NODE_ENV",
    "build": "tsc -p tsconfig.app.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "postinstall": "npm install --save-dev @types/react @types/react-dom",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "npm install && npm run build && npx serve -s build",
    "start:backend": "cd server && npm install && node server.js"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "concurrently": "^9.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^4.5.9"
  }
}
